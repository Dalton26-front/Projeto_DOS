@model Projeto_DOS.Models.Codcurso

@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="pt-br">
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <title>Excluir</title>
</head>
<body>
    <div class="container">
        <div class="container bg-success p-sm-2 text-sm-center">
            <h3>Tem certeza de que deseja excluir Curso - @Html.DisplayFor(model => model.CODCURSO)</h3>
        </div>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.CODCURSO)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CODCURSO)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.TURMA)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.TURMA)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.LISTA)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.LISTA)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.DATAMOV)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.DATAMOV)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.DISCIPLINA)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.DISCIPLINA)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.SUBDIS)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.SUBDIS)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.PROFESSOR)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.PROFESSOR)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.CODDIS)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CODDIS)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.CODPRO)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CODPRO)
            </dd>
        </dl>

        <form asp-action="Delete">
            <input type="hidden" asp-for="ID" />
            <input type="submit" value="Excluir" class="btn btn-danger" />
            <a asp-action="Index" class="btn btn-dark">Voltar Lista</a>
        </form>
    </div>
</body>
</html>
